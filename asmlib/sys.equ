section '.sys_data' writable
    sys_lseek   equ 19
    sys_creat   equ 8
    sys_close   equ 6
    sys_open    equ 5
    sys_write   equ 4
    sys_read    equ 3
    sys_exit    equ 1

section '.sys_rwx' writable
    sys_rwx_rdonly equ 0
    sys_rwx_wronly equ 1
    sys_rwx_rdwr   equ 2

section '.sys_seek' writable
    sys_seek_set equ 0
    sys_seek_cur equ 1
    sys_seek_end equ 2
